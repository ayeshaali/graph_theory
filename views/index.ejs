<!DOCTYPE html>
<html>
<head>
  <title>Networks</title>
  <script src="//d3js.org/d3.v3.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.js"></script>
  <link rel="stylesheet" href="style.css">
 </head>

<body>
  <div id="header">
    <h1>Trin Analytics</h1>
  </div>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.js"></script>
  <script src="//d3js.org/d3.v3.min.js"></script>
  <script id = "scriptJS" src="script.js"></script>
  <button id="reset" onclick="reload_js()">
    Reset
  </button>
  <div id="info">
  </div>
  <script>
    function reload_js() {
      console.log("remove");
      $('svg').remove();      
      $('<script>').attr('src', "script.js").appendTo('body');
    }
    
    var windowWidth = $(window).width();
    $('#header').css('width', windowWidth);
    $('#info').css('left', windowWidth*.72);
    $('#info').css('width', windowWidth*.25);
  </script>
 </body>
</html>
